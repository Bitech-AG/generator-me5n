{
  "name": "generator-me-ui-5-n",
  "version": "0.0.0",
  "description": "Generates fullstack openUI5 projects with mongodb, expressjs and nodejs (me5n)",
  "homepage": "www.bitech.ag",
  "author": {
    "name": "Richard Martens",
    "email": "richard.martens@bitech.ag",
    "url": "https://www.bitech.ag"
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "openui5",
    "ui5",
    "odata",
    "mongodb",
    "express",
    "node",
    "me5n",
    "yeoman-generator"
  ],
  "devDependencies": {
    "yeoman-test": "^8.1.0",
    "yeoman-assert": "^3.1.1",
    "coveralls": "^3.1.1",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.1",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-config-xo": "^0.43.1",
    "jest": "^29.7.0"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "yeoman-generator": "^6.0.1",
    "yosay": "^2.0.2"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "eslintConfig": {
    "extends": [
      "xo"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": "error"
    },
    "plugins": [
      "prettier"
    ]
  },
  "scripts": {
    "pretest": "eslint .",
    "test": "jest"
  },
  "repository": "bitech-ag/generator-me-ui-5-n",
  "license": "MIT"
}
